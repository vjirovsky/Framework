{if $paginator->pageCount && $paginator->pageCount > 1}
	<div class='pagination'>
		<ul>
			{foreach $steps as $step}
				{if $step == $paginator->page}
					<li class='active'>
						<a href='#'>{$step}</a>
					</li>

				{else}
					<li>
						<a n:href='this, page => $step'>{$step}</a>
					</li>
				{/}

				{if $iterator->nextValue > $step + 1}
					<li>
						<a href='#' class='dislabled'>â€¦</a>
					</li>
				{/}
			{/}
		</ul>
	</div>
{/}
